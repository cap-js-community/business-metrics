{
  "name": "bookshop",
  "dependencies": {
    "@cap-js/sqlite": "*",
    "@cap-js/telemetry": "*",
    "@opentelemetry/exporter-prometheus": "^0.57.2",
    "@opentelemetry/host-metrics": "*",
    "@opentelemetry/instrumentation-runtime-node": "*",
    "@sap/cds-mtxs": "*",
    "business-metrics": "*"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9"
  },
  "cds": {
    "requires": {
      "telemetry": {
        "kind": "telemetry-to-console",
        "metrics": {
        "enableBusinessMetrics": true,
        "exporter": {
                            "module": "@opentelemetry/exporter-prometheus",
                            "class": "PrometheusExporter",
                            "config": {
                                "host": "localhost",
                                "port": 4005,
                                "endpoint": "metrics"
                            }
                        }
      }
    }
  }
}
}
